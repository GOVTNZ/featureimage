<div class="feature-image">
	<div class="container">
		<% if $FeaturedImageText %>
			<p class="sr-only">$FeaturedImageText</p>
		<% end_if %>
		<div class="row">
			<div class="col-md-12">
				<% if $FeatureText %>
					<h1 class="feature-text">
						$FeatureText
					</h1>
				<% end_if %>
			</div>
		</div>
	</div>
</div>